{
  "name": "react-template",
  "version": "1.0.0",
  "private": true,
  "description": "A React Template Project",
  "author": "Jayson Wu <wulingjie991@outlook.com>",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "test": "jest --watchAll",
    "lint": "eslint src --ext .js",
    "prettier": "prettier --write src/**/*.js",
    "build": "webpack --config config/webpack.prod.js",
    "start": "webpack serve --config config/webpack.dev.js",
    "docker:dev": "docker build -f Dockerfile.dev -t react-template . && docker run -v ${PWD}:/app -v /app/node_modules -p 8080:8080 --rm react-template",
    "docker:deploy": "docker build -f Dockerfile.prod -t react-template-builder . && docker run -it -p 80:80 --rm react-template-builder"
  },
  "keywords": [
    "jest",
    "sass",
    "babel",
    "react",
    "docker",
    "eslint",
    "webpack",
    "template",
    "node-sass"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "autoprefixer": "^10.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "core-js": "^3.8.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
