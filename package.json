{
  "name": "react-template",
  "version": "1.0.0",
  "private": true,
  "description": "A React Template Project",
  "author": "Jayson Wu <wulingjie991@outlook.com>",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "test": "jest --watchAll",
    "prepare": "husky install",
    "lint": "eslint ./ --ext .js,.jsx",
    "lint:fix": "eslint ./ --fix --ext .js,.jsx",
    "prettify": "prettier --write src/**/*.{js,jsx}",
    "build": "webpack --config config/webpack.prod.js",
    "start": "webpack serve --config config/webpack.dev.js",
    "docker:dev": "docker build -f Dockerfile.dev -t dev:builder . && docker run -v ${PWD}:/app -v /app/node_modules -p 8080:8080 --rm dev:builder",
    "docker:deploy": "docker build -f Dockerfile.prod -t prod:builder . && docker run -it -p 80:80 --rm prod:builder"
  },
  "keywords": [
    "jest",
    "sass",
    "test",
    "babel",
    "react",
    "enzyme",
    "docker",
    "eslint",
    "postcss",
    "webpack",
    "prettier",
    "template",
    "unit-test",
    "autoprefixer",
    "react-template"
  ],
  "dependencies": {
    "core-js": "^3.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/eslint-plugin": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "^5.2.2",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.5.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.10.0",
    "sass": "^1.32.10",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
